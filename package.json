    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^6.3.0",
    "microbundle-crl": "^0.13.10",
    "mini-css-extract-plugin": "^2.9.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react-scripts": "^5.0.1",
    "webpack": "^5.99.7",
    "webpack-cli": "^6.0.1"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "axios": "^1.6.7",
    "isomorphic-dompurify": "^2.4.0",
    "js-cookie": "^3.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
